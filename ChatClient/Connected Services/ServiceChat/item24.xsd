<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/wcf_chat" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/wcf_chat" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="ServiceChat.GameAction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="Rock" />
      <xs:enumeration value="Paper" />
      <xs:enumeration value="Scissors" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ServiceChat.GameAction" nillable="true" type="tns:ServiceChat.GameAction" />
  <xs:simpleType name="ServiceChat.GameResult">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="WinRock" />
      <xs:enumeration value="WinPaper" />
      <xs:enumeration value="WinScissors" />
      <xs:enumeration value="Draw" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ServiceChat.GameResult" nillable="true" type="tns:ServiceChat.GameResult" />
</xs:schema>